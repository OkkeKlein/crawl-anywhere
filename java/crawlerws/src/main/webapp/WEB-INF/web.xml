<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <display-name>crawlerws</display-name>
    <description>crawlerws</description>
    <servlet>
        <servlet-name>crawlerws</servlet-name>
		<servlet-class>fr.eolya.crawler.ws.CrawlerWS</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>crawlerws</servlet-name>
        <url-pattern>/crawlerws</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>crawlerws</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
</web-app>